{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "build",
    "rootDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true, 
    "isolatedModules": true,
    "moduleResolution":"node",
    "sourceMap": false,
    
  }
}
